{
  "name": "modern-retail-app",
  "version": "0.0.1",
  "description": "A modern retail app written in Node.JS",
  "main": "./bin/www",
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "expect-ct": "^0.2.0",
    "express": "~4.16.0",
    "express-enforces-ssl": "^1.1.0",
    "hbs": "^4.0.4",
    "helmet": "^3.18.0",
    "http-errors": "~1.6.2",
    "js-yaml": "3.13.1",
    "lodash": "^4.17.14",
    "morgan": "~1.9.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@commitlint/prompt-cli": "^8.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^1.1.3",
    "jsdoc": "^3.6.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.1"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha tests/* --timeout 10000",
    "makedocs": "rm -rf ./code-docs; ./node_modules/.bin/jsdoc app.js routes/* --destination ./code-docs",
    "history": "bash ./scripts/history.sh",
    "lint": "./node_modules/.bin/eslint --color --fix config/* routes/* services/*",
    "start": "npm run dev",
    "dev": "export NODE_STATIC_PORT=3000; export NODE_ENV=development; export NODE_RUNNER=nodemon; ./node_modules/.bin/nodemon ./bin/www",
    "prod": "export NODE_ENV=production; node ./bin/www &",
    "killserver": "bash ./bin/killserver",
    "profile": "bash ./scripts/profile.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cupcakeIBM/mock"
  },
  "keywords": [
    "modern",
    "retail",
    "app"
  ],
  "husky": {
    "pre-commit": "npm run lint;",
    "pre-push": "npm run test;",
    "commit-msg": "./node_modules/.bin/commitlint -E HUSKY_GIT_PARAMS"
  },
  "author": "",
  "license": "ISC"
}
